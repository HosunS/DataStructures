graph {
graph [layout=circo]
a [style="filled", fillcolor=lightsalmon]
c [style="filled", fillcolor=lightsalmon]
x [style="filled", fillcolor=lightpink]
b [style="filled", fillcolor=lightsalmon]
d [style="filled", fillcolor=lightsalmon]
